{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es5",
		"strict": true,
		"strictNullChecks": true,
		"strictPropertyInitialization": true,
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"declaration": false,
		"sourceMap": true,
		"jsx": "react",
		"lib": ["dom", "es2015", "es2016", "es2017", "esnext"]
	},
	"exclude": ["node_modules"]
}
